{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@pages/*": ["pages/*"],
      "@assets/*": ["assets/*"],
      "@config/*": ["config/*"],
      "@constants/*": ["constants/*"],
      "@redux/*": ["redux/*"],
      "@utility/*": ["utility/*"],
      "@services/*": ["services/*"],
      "@models/*": ["models/*"],
      "@layouts/*": ["layouts/*"],
    }
  },
  "include": ["src"]
}
